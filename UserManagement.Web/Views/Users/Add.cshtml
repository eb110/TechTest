@model UserManagement.Web.Models.Users.UserModel

<h1>Add Student</h1>

<form method="post">

    <div class="mt-3">
        <label class="form-label">Forename</label>
        <input type="text" class="form-control" asp-for="Forename" />
    </div>

    <div class="mt-3">
        <label class="form-label">Surname</label>
        <input type="text" class="form-control" asp-for="Surname" />
    </div>

    <div class="mt-3">
        <label class="form-label">Email</label>
        <input type="email" class="form-control" asp-for="Email" />
    </div>

    <div class="mt-3">
        <label class="form-label">DOB</label>
        <input type="date" class="form-control" asp-for="DateOfBirth" />
    </div>

    <div class="mt-3">
        <button type="submit" class="btn btn-primary">Save</button>
    </div>

</form>
